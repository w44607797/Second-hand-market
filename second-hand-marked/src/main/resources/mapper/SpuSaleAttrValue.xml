<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mar.bean.mapper.SpuSaleAttrValue">
    <!--可以使用一堆sql-->
    <select id="selectBySpuIdAndAttrId" resultType="com.mar.bean.dto.SpuSaleAttrValueDTO">
        select * from spu_sale_attr_value where spuId=#{spuId} and baseSaleAttrId=#{baseSaleAttrId}
    </select>
</mapper>